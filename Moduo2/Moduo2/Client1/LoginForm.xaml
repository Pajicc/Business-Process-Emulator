        Title="LoginForm" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textbox1" HorizontalAlignment="Left" Height="23" Margin="136,59,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="39,59,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="42,115,0,0" VerticalAlignment="Top" />
        <Button Content="Ok" HorizontalAlignment="Left" Margin="42,214,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="181,214,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <PasswordBox HorizontalAlignment="Left" Height="22" Margin="136,119,0,0" VerticalAlignment="Top" Width="120" Name="textbox2"/>

    </Grid>
